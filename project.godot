; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Area2D",
"class": "Droppable",
"language": "GDScript",
"path": "res://droppables/droppable.gd"
}, {
"base": "Entity",
"class": "Enemy",
"language": "GDScript",
"path": "res://enemies/enemy.gd"
}, {
"base": "KinematicBody2D",
"class": "Entity",
"language": "GDScript",
"path": "res://engine/entity.gd"
}, {
"base": "Node2D",
"class": "Item",
"language": "GDScript",
"path": "res://engine/item.gd"
}, {
"base": "Node2D",
"class": "Switch",
"language": "GDScript",
"path": "res://engine/switch.gd"
}, {
"base": "Reference",
"class": "entity_helper",
"language": "GDScript",
"path": "res://engine/helper_scipts/entity_helper.gd"
} ]
_global_script_class_icons={
"Droppable": "",
"Enemy": "",
"Entity": "",
"Item": "",
"Switch": "",
"entity_helper": ""
}

[application]

config/name="TetraForce"
run/main_scene="res://engine/lobby.tscn"
config/icon="res://icon.png"

[autoload]

sfx="*res://engine/sound.gd"
global="*res://engine/global.gd"
network="*res://engine/network.gd"
screenfx="*res://engine/screenfx.tscn"
settings="*res://engine/settings.gd"
controller="*res://engine/controller.gd"

[debug]

gdscript/warnings/enable=false

[display]

window/size/width=256
window/size/height=144
window/size/test_width=1280
window/size/test_height=720
window/stretch/mode="2d"
window/stretch/aspect="keep"

[editor_plugins]

enabled=PoolStringArray( "vnen.tiled_importer" )

[importer_defaults]

texture={
"compress/bptc_ldr": 0,
"compress/hdr_mode": 0,
"compress/lossy_quality": 0.7,
"compress/mode": 0,
"compress/normal_map": 0,
"detect_3d": false,
"flags/anisotropic": false,
"flags/filter": false,
"flags/mipmaps": false,
"flags/repeat": 0,
"flags/srgb": 2,
"process/HDR_as_SRGB": false,
"process/fix_alpha_border": true,
"process/invert_color": false,
"process/premult_alpha": false,
"size_limit": 0,
"stream": false,
"svg/scale": 1.0
}
vnen.tiled_importer={
"add_background": true,
"collision_layer": 1,
"custom_properties": true,
"embed_internal_images": false,
"image_flags": 7,
"post_import_script": "res://tiled/import.gd",
"save_tiled_properties": true,
"tile_metadata": true,
"uv_clip": true
}

